{
    "name": "NightEXT",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": " Meet NightEXT, a dark theme for Google Chrome. It's free and open source. ",
    "background": {
        "scripts": [
            "js/popup.js",
            "js/settings.js"
        ],
        "persisten": true
    },
    "icons":{
        "128":"resources/logo.png"
    },
    "browser_action": {
        "default_title": "NightEXT",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}